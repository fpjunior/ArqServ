{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/arqserv-frontend/index.html"
    }
  ],
  "outputDirectory": "frontend/dist/arqserv-frontend"
}
